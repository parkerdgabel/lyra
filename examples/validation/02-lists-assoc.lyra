(*
  02-lists-assoc.lyra â€” Lists, associations, indexing with Part and Lookup
*)

(* Lists *)
{1,2,3};                 (* => {1,2,3} *)
{1,2,3}[[2]];            (* 1-based index => 2 *)

(* Associations with symbol or string keys *)
<|a->1, "b"->2|>;        (* => <|a->1, b->2|> *)
Part[<|a->1, b->2|>, "b"];  (* => 2 *)

(* Dot property and method chaining *)
<|name->"ann"|>.name;    (* => "ann" via Lookup *)

